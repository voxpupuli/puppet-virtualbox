{
  "name": "puppet-virtualbox",
  "version": "1.7.4-rc0",
  "author": "Vox Pupuli",
  "summary": "Installs Virtualbox on RHEL and Debian-like systems",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-virtualbox",
  "project_page": "https://github.com/voxpupuli/puppet-virtualbox",
  "issues_url": "https://github.com/voxpupuli/puppet-virtualbox/issues",
  "tags": [
    "virtualbox",
    "virtualization"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.8.7 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.8.0 <5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=2.1.0 <3.0.0"
    },
    {
      "name": "stahnma/epel",
      "version_requirement": ">=1.0.0 <2.0.0"
    }
  ]
}
