{
  "name": "puppet-virtualbox",
  "version": "2.2.1",
  "author": "Vox Pupuli",
  "summary": "Installs Virtualbox on RHEL and Debian-like systems",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-virtualbox",
  "project_page": "https://github.com/voxpupuli/puppet-virtualbox",
  "issues_url": "https://github.com/voxpupuli/puppet-virtualbox/issues",
  "tags": [
    "virtualbox",
    "virtualization"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 6.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.1.0 < 7.0.0"
    },
    {
      "name": "stahnma/epel",
      "version_requirement": ">= 1.0.0 <2.0.0"
    }
  ]
}
